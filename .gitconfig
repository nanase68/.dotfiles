[alias]
  b   = branch
  ba  = branch -all
  c   = checkout
  cb  = checkout -b
  cm  = checkout master
 coa  = commit --amend
  ms  = merge --squash
  ri  = rebase -i
  rp  = remote prune origin
  st  = stash
  stp = stash pop
  p   = pull
  f   = fetch
  s   = status
  bl  = blame
  g   = grep -nI --color

[color]
  ui = auto

[core]
  editor = vim
